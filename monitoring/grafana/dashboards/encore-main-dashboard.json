          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "yaxes": [
          {
            "format": "currencyUSD",
            "label": "Revenue"
          }
        ]
      },
      {
        "id": 2,
        "title": "Active Users",
        "type": "graph",
        "targets": [
          {
            "refId": "A",
            "target": "active_users",
            "type": "timeserie"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Song Plays",
        "type": "graph",
        "targets": [
          {
            "refId": "A",
            "target": "song_plays",
            "type": "timeserie"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Queue Length",
        "type": "graph",
        "targets": [
          {
            "refId": "A",
            "target": "queue_length",
            "type": "timeserie"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
