    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsdom": "^23.2.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "qs": "^6.11.2",
    "redis": "^4.6.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1",
    "uuid": "^9.0.1",
