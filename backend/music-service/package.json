{
  "name": "encore-music-service",
  "version": "1.0.0",
  "description": "Encore Music Service - Handles song management, external API integrations (YouTube/Spotify), and queue management",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "rimraf dist && tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist node_modules/.cache",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "encore",
    "music",
    "queue",
    "youtube",
    "spotify",
    "api",
    "microservice",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "Encore Team",
  "license": "MIT",
